import turtle
t = turtle.Turtle()

t.home()
t.clear()

for i in range(3):
  t.forward(100)
  t.left(110)
  
 print('hello')

def func(v):
  i = v + 3
  return i
  
func(5)

def total(n):
  s = 0
  for i in range(1, n + 1):
    s += i
  return s

def total(n):
  s = 0
  for i in range(1, n + 1):
    s += 1
  return s

total(5)

total(10)

