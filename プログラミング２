 import turtle
t = turtle.Turtle()
 
t.home()
t.clear()
 
for i in range(4):
  t.up()
  t.forward(20)
  t.down()
  for x in range(4):
    t.forward(100)
    t.left(90)
  t.left(-90)
   
print('hello')

